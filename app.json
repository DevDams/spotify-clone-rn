{
  "expo": {
    "name": "spotify",
    "slug": "spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-spotify.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-screen-spotify.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-spotify.png",
        "backgroundColor": "#1ED760"
      }
    },
    "web": {
      "favicon": "./assets/icon-spotify.png"
    }
  }
}
